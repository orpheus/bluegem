# Database Configuration
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/bluegem

# Redis Configuration  
REDIS_URL=redis://localhost:6379

# API Keys
OPENAI_API_KEY=your-openai-api-key-here
FIRECRAWL_API_KEY=your-firecrawl-api-key-here

# LLM Configuration
LLM_MODEL=gpt-4
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=2000

# Quality Settings
QUALITY_AUTO_APPROVE_THRESHOLD=0.8
CONFIDENCE_MINIMUM=0.6

# Processing Limits
MAX_CONCURRENT_SCRAPES=5
RATE_LIMIT_REQUESTS_PER_MINUTE=10

# Cache Settings
REDIS_CACHE_TTL=3600
REDIS_SIMILARITY_THRESHOLD=0.85

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/agent.log

# Export Settings
EXPORT_DIRECTORY=exports/
REVIT_TEMPLATE_PATH=templates/revit_template.csv